  <pattern name="aepd.13">
    <control evl="aepd:2021" control="13" />
  </pattern>

  <pattern name="aepd.13.graph">
    <graph what="evl.valuation"
      mode="percent"
      graph="bars.horizontal"
      width="15cm" >
      <foreach_control evl="aepd:2021"
        pattern="13.*"
        applies="yes" 
        text="[code] name" />
      <foreach_phase
        text="[code] name" />
    </graph>
    <legend/>
  </pattern>

  <pattern name="aepd.13.table">
    <evl.valuation evl="aepd:2021" 
      pattern="13.*"
      mode="maturity"
      applies="all"
      diff="on"
      comments="3"
    />
  </pattern>

  <pattern name="aepd.c.13.table">
    <evl.compliance evl="aepd:2021" 
      pattern="13.*"
      mode="percent"
      />
  </pattern>

  <pattern name="evl.13.domain">
    <domain />
    <nl/>

    <graph what="evl.valuation"
      mode="percent"
      graph="radar.lines"
      width="15cm" >
      <foreach_control evl="aepd:2021"
        pattern="13.*"
        applies="yes" 
        text="[code]" />
      <foreach_phase
        text="[code] name" />
    </graph>
    <nl/>
    <legend/>
    <nl/>

    <table what="evl.valuation"
      mode="maturity">
      <foreach_control evl="aepd:2021" 
	pattern="13.*"
	applies="all" />
      <foreach_phase
        text="[code]" />
    </table>
    <nl/>
    <comments slice="domain" >
      <foreach_control evl="aepd:2021" 
	pattern="13.*"
	applies="all" />
    </comments>
  </pattern>

</patterns>

