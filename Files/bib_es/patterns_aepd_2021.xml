<?xml version="1.0" encoding="UTF-8" ?>
<patterns>

  <pattern name="asset.valuation.graph">
    <graph what="value.own"
           graph="bars.horizontal"
           width="15cm" >
      <foreach_asset family="essential" />
      <foreach_dimension />
    </graph>
    <legend />
  </pattern>

  <pattern name="asset.valuation.table">
    <asset.valuation />
  </pattern>

  <pattern name="risk.deflected.graph">
    <foreach_asset family="essential">
      <nl /><nl /><asset /><nl /><nl /> 
      <graph what="risk.deflected" 
          graph="radar.lines" with="15cm" >
        <foreach_phase text="code"/>
        <foreach_dimension text="[code] name"/>
      </graph>
      <nl/>
      <legend />
    </foreach_asset>
  </pattern>

  <pattern name="risk.deflected.table">
    <foreach_asset family="essential">
      <nl /><nl /><asset /><nl /><nl /> 
      <table what="risk.deflected" >
        <foreach_phase text="code"/>
        <foreach_dimension text="[code]"/>
      </table>
    </foreach_asset>
  </pattern>


  <pattern name="aepd.1">
    <control evl="aepd:2021" control="1" />
  </pattern>

  <pattern name="aepd.1.graph">
    <graph what="evl.valuation"
      mode="percent"
      graph="bars.horizontal"
      width="15cm" >
      <foreach_control evl="aepd:2021"
        pattern="1.*"
        applies="yes" 
        text="[code] name" />
      <foreach_phase
        text="[code] name" />
    </graph>
    <legend/>
  </pattern>

  <pattern name="aepd.1.table">
    <evl.valuation evl="aepd:2021" 
      pattern="1.*"
      mode="maturity"
      applies="all"
      diff="on"
      comments="3"
    />
  </pattern>

  <pattern name="aepd.c.1.table">
    <evl.compliance evl="aepd:2021" 
      pattern="1.*"
      mode="percent"
      />
  </pattern>

  <pattern name="evl.1.domain">
    <domain />
    <nl/>

    <graph what="evl.valuation"
      mode="percent"
      graph="radar.lines"
      width="15cm" >
      <foreach_control evl="aepd:2021"
        pattern="1.*"
        applies="yes" 
        text="[code]" />
      <foreach_phase
        text="[code] name" />
    </graph>
    <nl/>
    <legend/>
    <nl/>

    <table what="evl.valuation"
      mode="maturity">
      <foreach_control evl="aepd:2021" 
	pattern="1.*"
	applies="all" />
      <foreach_phase
        text="[code]" />
    </table>
    <nl/>
    <comments slice="domain" >
      <foreach_control evl="aepd:2021" 
	pattern="1.*"
	applies="all" />
    </comments>
  </pattern>


